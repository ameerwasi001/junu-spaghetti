open setName(a{.pet: b{.name: String}}, String) => b{.name: String} for a{.pet: b{.name: String}}

impl setName(person: Person, s: String) => {
    person.name = s
    return person.pet
} for Person

impl setName(cat: {pet: Person}, s: String) => {
    cat.pet.name = s
    return cat.pet
} for {pet: Person}
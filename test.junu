let ftr = (a: String, b: {name: String, species: String} | Pet | Person) => String {
    if b notis Person {
        return b.species + " " + b.name
    } else {
        return b.pet.species + " " + b.name
    }
    return b.name + a
}

let flr = (x: Pet | Person) => String {
    if x is Person {
        return ftr(x.name, x)
    }
    return ftr(x.species, x)
}

